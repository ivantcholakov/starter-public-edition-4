
{% set image_count = images|count %}
{% if image_count > 0 %}

{{ css('lib/swiper/swiper.min.css') }}

    <style>

    .swiper-button-prev {
        background-image: url("data:image/png;base64,") !important;
        width: 24px !important;
        height: 80px !important;
        margin-top: -40px !important;
        background-size: 24px 80px !important;
        outline: none !important;
    }

    .swiper-button-next {
        background-image: url("data:image/png;base64,") !important;
        width: 24px !important;
        height: 80px !important;
        margin-top: -40px !important;
        background-size: 24px 80px !important;
        outline: none !important;
    }

    .swiper-pagination-bullet-active {
        background: #5f5f5f !important;
    }

    </style>

    <style>

        #gallery-top-container {
            position: relative;
            width: 100%;
            height: auto;
        }

        #gallery-top {
            width: 100%;
            height: auto;
            padding-left: 1em;
            padding-right: 1em;
        }

        #gallery-top .swiper-slide {
            width: 100%;
            padding-bottom: 66.67%;
            background-repeat: no-repeat;
            background-size: contain;
            background-position: center;
        }

        #gallery-top-swiper-button-prev, #gallery-top-swiper-button-next {
            z-index: 9 !important;
        }

        #gallery-thumbs-container {
            position: relative;
            width: 100%;
            height: auto;
        }

        #gallery-thumbs {
            width: 100%;
            height: 160px;
            box-sizing: border-box;
            padding: 10px 0;
        }

        #gallery-thumbs .swiper-slide {
            width: 100%;
            height: auto;
            background-repeat: no-repeat;
            background-size: contain;
            background-position: center;
            opacity: 0.5;
        }

        #gallery-thumbs .swiper-slide-thumb-active {
            opacity: 1;
        }

        #gallery-thumbs-swiper-button-prev, #gallery-thumbs-swiper-button-next {
            z-index: 9 !important;
        }

        @media (min-width: 641px) {
            #gallery-top {
                margin-left: 42px !important;
                margin-right: 42px !important;
                width: auto !important;
            }
            #gallery-thumbs {
                margin-left: 42px !important;
                margin-right: 42px !important;
                width: auto !important;
            }
        }

        @media (max-width: 640px) {
            #gallery-top-swiper-button-prev, #gallery-top-swiper-button-next {
                display: none !important;
            }
            #gallery-thumbs-swiper-button-prev, #gallery-thumbs-swiper-button-next {
                display: none !important;
            }
        }

    </style>

    <style>

        #example2-slider-container {
            position: relative;
            width: 100%;
            height: auto;
            background-color: #fff;
        }

        #example2-slider {
            width: 100%;
            height: auto;
            padding-left: 1em;
            padding-right: 1em;
        }

        #example2-slider .swiper-slide {
            text-align: center;
            height: auto;
            background: #fff;
            /* Center slide text vertically */
            display: -webkit-box;
            display: -ms-flexbox;
            display: -webkit-flex;
            display: flex;
            -webkit-box-pack: center;
            -ms-flex-pack: center;
            -webkit-justify-content: center;
            justify-content: center;
            -webkit-box-align: center;
            -ms-flex-align: center;
            -webkit-align-items: center;
            align-items: center;
        }

        #example2-swiper-button-prev, #example2-swiper-button-next {
            z-index: 9 !important;
        }

        #example2-slider .example2-item {
            width: 300px !important;
            height: auto !important;
        }

        @media (min-width: 641px) {
            #example2-slider {
                margin-left: 42px !important;
                margin-right: 42px !important;
                width: auto !important;
            }
        }

        @media (max-width: 640px) {
            #example2-swiper-button-prev, #example2-swiper-button-next {
                display: none !important;
            }
        }

    </style>

    <style>

        #example3-slider {
            width: 100%;
            height: 460px;
        }

        #example3-slider .swiper-slide {
            height: 100%;
            width: auto;
            background-repeat: no-repeat;
            background-size: contain;
            background-position: center;
        }

    </style>

{% endif %}
