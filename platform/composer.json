        "symfony/polyfill-php81": "1.28.0",
        "symfony/polyfill-php82": "1.28.0",
        "symfony/polyfill-php83": "1.28.0",
        "symfony/polyfill-ctype": "1.28.0",
        "symfony/polyfill-mbstring": "1.28.0",
        "symfony/polyfill-iconv": "1.28.0",
        "symfony/polyfill-intl-grapheme": "1.28.0",
        "symfony/polyfill-intl-idn": "1.28.0",
        "symfony/polyfill-intl-icu": "1.28.0",
        "symfony/polyfill-intl-messageformatter": "1.28.0",
        "symfony/polyfill-intl-normalizer": "1.28.0",
        "symfony/polyfill-uuid": "1.28.0",
        "symfony/deprecation-contracts": "3.0.2",
        "symfony/string": "6.0.19",
        "symfony/options-resolver": "6.0.19",
        "symfony/process": "6.0.19",
        "symfony/filesystem": "6.0.19",
        "symfony/finder": "6.0.19",
        "psr/cache": "3.0.0",
        "fg/multiplayer": "1.2.1",
        "voku/portable-utf8": "6.0.13",
