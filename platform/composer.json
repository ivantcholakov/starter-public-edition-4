{
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-types": ["library", "component"],
        "installer-paths": {
            "../public/assets/composer-asset/{$vendor}/{$name}/": [
                "type:component"
            ]
        }
    },
    "require": {
        "php": ">=7.3.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "codeigniter/framework": "3.1.13",
        "symfony/polyfill-php73": "^1.20",
        "symfony/polyfill-php74": "^1.20",
        "symfony/polyfill-php80": "^1.20",
        "symfony/polyfill-ctype": "^1.20",
        "symfony/polyfill-mbstring": "^1.20",
        "symfony/polyfill-iconv": "^1.20",
        "symfony/polyfill-intl-grapheme": "^1.20",
        "symfony/polyfill-intl-idn": "^1.20",
        "symfony/polyfill-intl-icu": "^1.20",
        "symfony/polyfill-intl-messageformatter": "^1.20",
        "symfony/polyfill-intl-normalizer": "^1.20",
        "symfony/polyfill-uuid": "^1.20",
        "symfony/deprecation-contracts": "2.5.2",
        "symfony/string": "5.4.12",
        "symfony/options-resolver": "5.4.11",
        "symfony/process": "5.4.11",
        "symfony/filesystem": "5.4.12",
        "symfony/finder": "5.4.11",
        "psr/cache": "1.0.1",
        "fg/multiplayer": "1.2.1",
        "voku/portable-utf8": "6.0.9",
        "voku/anti-xss": "4.1.39",
        "erusev/parsedown": "1.7.4",
        "erusev/parsedown-extra": "0.8.1",
        "pixel418/markdownify": "2.3.1",
        "mustache/mustache": "2.14.2",
        "netcarver/textile": "3.7.7",
        "twig/twig": "3.4.3",
        "twig/string-extra": "3.4.0",
        "salesforce/handlebars-php": "3.0.0",
        "scssphp/scssphp": "1.11.0",
        "matthiasmullie/minify": "1.3.69",
        "t1st3/php-json-minify": "0.6.2",
        "whichbrowser/parser": "2.1.7",
        "ezyang/htmlpurifier": "4.14.0",
        "ivantcholakov/codeigniter-phpmailer": "1.5.0",
        "league/oauth2-google": "4.0.0",
        "hayageek/oauth2-yahoo": "2.0.5",
        "stevenmaguire/oauth2-microsoft": "2.2.0",
        "intervention/image": "2.7.2",
        "yohang88/letter-avatar": "1.0.5",
        "athlon1600/php-proxy": "5.2.0",
        "guzzlehttp/guzzle": "7.5.0",
        "components/jquery": "3.6.0",
        "components/modernizr": "3.5.0",
        "components/webfontloader": "1.6.3",
        "components/highlightjs": "9.16.2",
        "scrivo/highlight.php": "9.18.1.9",
        "desandro/imagesloaded": "4.1.4",
        "desandro/masonry": "4.2.2"
    }
}
